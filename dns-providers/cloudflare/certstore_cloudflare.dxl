<item name='Version'><text>1.1</text></item>
<item name='Author'><text>HCL</text></item>
<item name='ReferenceURL'><text/></item>
<item name='StatusFormula'><text>@if (retJSON_Add.success = "true"; 200; 400)</text></item>
<item name='cfg_URL'><text>https://api.cloudflare.com/client/v4/zones</text></item>
<item name='ReqLookupUrlFormula'><text>@if (cfg_DnsZone = ""; cfg_URL + "?name="+param_RegisteredDomain; "")</text></item>
<item name='ReqLookupHeaderFormula'><text>@if (cfg_AuthToken = ""; ("X-Auth-Email: " + cfg_InternetAddress) : ( "X-Auth-Key: " + cfg_AuthKey ); "Authorization: Bearer " + cfg_AuthToken ) : ( "Content-Type: application/json" )</text></item>
<item name='ReqLookupPostDataFormula'><text/></item>
<item name='ReqLookupCustomFormula'><text>@if (cfg_DnsZone = ""; @SetField ("cfg_DnsZone"; retJSON_Lookup.result.0.id ); ""); cfg_DnsZone</text></item>
<item name='ReqQryAddUrlFormula'><text/></item>
<item name='ReqQryAddHeaderFormula'><text/></item>
<item name='ReqQryAddPostDataFormula'><text/></item>
<item name='ReqAddUrlFormula'><text>cfg_URL +"/"+ cfg_DnsZone + "/dns_records/"</text></item>
<item name='ReqAddHeaderFormula'><text>@if (cfg_AuthToken = ""; ("X-Auth-Email: " + cfg_InternetAddress) : ( "X-Auth-Key: " + cfg_AuthKey ); "Authorization: Bearer " + cfg_AuthToken ) : ( "Content-Type: application/json" )</text></item>
<item name='ReqAddPostDataFormula'><text>'{"type":"TXT","name":"' + param_DnsTxtName+ '","content":"' + param_DnsTxtValue + '","ttl":120}'</text></item>
<item name='ReqQryDelUrlFormula'><text/></item>
<item name='ReqQryDelHeaderFormula'><text/></item>
<item name='ReqQryDelPostDataFormula'><text/></item>
<item name='ReqDelUrlFormula'><text>@if (retJSON_Add.result.id = ""; ""; cfg_URL +"/"+ cfg_DnsZone + "/dns_records/" + retJSON_Add.result.id)</text></item>
<item name='ReqDelHeaderFormula'><text>@if (cfg_AuthToken = ""; ("X-Auth-Email: " + cfg_InternetAddress) : ( "X-Auth-Key: " + cfg_AuthKey ); "Authorization: Bearer " + cfg_AuthToken ) : ( "Content-Type: application/json" )</text></item>
